<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>AaronSalisbury.DotNetFrameworkToolkit</id>
    <version>0.1.0</version>
    <authors>Aaron Salisbury</authors>
    <license type="expression">MIT</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <icon>nuget-package-icon.png</icon>
    <readme>readme.md</readme>
    <projectUrl>https://github.com/aaron-salisbury/DotNetFrameworkToolkit</projectUrl>
    <description>Common C# app development and shim code for legacy .Net Framework 2.0 projects.</description>
    <releaseNotes>In-development / pre-alpha.</releaseNotes>
    <copyright>Â© Aaron Salisbury</copyright>
    <tags>app helpers legacy netframework</tags>
    <repository type="git" url="https://github.com/aaron-salisbury/DotNetFrameworkToolkit" />
    <dependencies>
      <group targetFramework="net20">
        <dependency id="PatternsAndPractices.Cryptography" version="2.0.0" exclude="Build,Analyzers" />
        <dependency id="PatternsAndPractices.Logging" version="2.0.0" exclude="Build,Analyzers" />
        <dependency id="PatternsAndPractices.Unity" version="1.2.0" exclude="Build,Analyzers" />
        <dependency id="System.Data.SqlServerCe_unofficial" version="4.0.8482.1" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\readme.md" target="" />
    <file src="..\..\content\nuget-package-icon.png" target="" />
    <file src="bin\Release\**\*.dll" target="lib\net20" />
    <file src="bin\Release\**\*.xml" target="lib\net20" />
    <file src="bin\Release\**\*.pdb" target="lib\net20" />
    <file src="**\*.cs" target="src" />
  </files>
</package>